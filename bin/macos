#!/usr/bin/env bash

script_dir=$(dirname $(realpath $0))

brew --version &> /dev/null || /usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"

brew install fish \
  git \
  jq \
  neovim \
  pyenv \
  rbenv \
  the_silver_searcher

"$script_dir/symlinks"